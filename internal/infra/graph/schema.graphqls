type Order {
    id: String!
    Price: Float!
    Tax: Float!
    FinalPrice: Float!
}

input OrderInput {
    id : String!
    Price: Float!
    Tax: Float!
}

type Query {
    orders(page: Int = 1, perPage: Int = 10): [Order!]! @paginate
}

type Mutation {
    createOrder(input: OrderInput): Order
}